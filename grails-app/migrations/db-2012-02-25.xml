<?xml version="1.0" encoding="UTF-8" standalone="no"?>

<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-2.0.xsd">
  <changeSet id="20120225-001" author="dellermann">
    <renameTable oldTableName="user" newTableName="user_data"/>
    <renameTable oldTableName="user_settings" newTableName="user_data_settings"/>
  </changeSet>
    
  <changeSet id="20120225-002" author="dellermann">
    <renameColumn tableName="calendar_event" oldColumnName="start"
                  newColumnName="start_time" columnDataType="datetime"/>
    <renameColumn tableName="calendar_event" oldColumnName="end"
                  newColumnName="end_time" columnDataType="datetime"/>
  </changeSet>
</databaseChangeLog>
