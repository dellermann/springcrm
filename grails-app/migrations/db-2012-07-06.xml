<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-2.0.xsd">
  <changeSet author="d.ellermann@amc-world.de" id="20120706-1">
    <dropNotNullConstraint tableName="project" columnName="description" 
      columnDataType="longtext"/>
  </changeSet>
  <changeSet author="d.ellermann@amc-world.de" id="20120706-2">
    <createIndex tableName="calendar_event" indexName="end_time">
      <column name="end_time"/>
    </createIndex>
    <createIndex tableName="calendar_event" indexName="start_time">
      <column name="start_time"/>
    </createIndex>
    <createIndex tableName="calendar_event" indexName="recurrence_type">
      <column name="recurrence_type"/>
    </createIndex>
    <createIndex tableName="reminder" indexName="next_reminder">
      <column name="next_reminder"/>
    </createIndex>
  </changeSet>
  <changeSet author="d.ellermann@amc-world.de" id="20120706-3">
    <createIndex tableName="invoicing_transaction" indexName="class">
      <column name="class"/>
    </createIndex>
    <createIndex tableName="sel_value" indexName="class">
      <column name="class"/>
    </createIndex>
  </changeSet>
</databaseChangeLog>
