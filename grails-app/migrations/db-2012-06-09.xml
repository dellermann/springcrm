<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-2.0.xsd">
  <changeSet author="d.ellermann@amc-world.de" id="20120609-1">
    <addColumn tableName="google_data_sync_status">
      <column name="etag" type="varchar(255)">
        <constraints nullable="false" />
      </column>
    </addColumn>
    <createIndex indexName="type" tableName="google_data_sync_status">
      <column name="type" />
    </createIndex>
    <dropIndex indexName="deleted" tableName="google_data_sync_status"/>
  </changeSet>

