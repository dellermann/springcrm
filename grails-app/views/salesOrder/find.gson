import org.amcworld.springcrm.SalesOrder
import org.amcworld.springcrm.SeqNumberService

model {
    List<SalesOrder> salesOrderList
    SeqNumberService seqNumberService
}

json(salesOrderList) { SalesOrder salesOrder ->
    id salesOrder.id
    number seqNumberService.getFullNumber(salesOrder)
    name salesOrder.subject
    fullName salesOrder.fullName
}
