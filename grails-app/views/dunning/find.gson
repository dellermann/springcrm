import org.amcworld.springcrm.Dunning
import org.amcworld.springcrm.SeqNumberService

model {
    List<Dunning> dunningList
    SeqNumberService seqNumberService
}

json(dunningList) { Dunning dunning ->
    id dunning.id
    number seqNumberService.getFullNumber(dunning)
    name dunning.subject
    fullName dunning.fullName
}
