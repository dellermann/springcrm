import org.amcworld.springcrm.Quote
import org.amcworld.springcrm.SeqNumberService

model {
    List<Quote> quoteList
    SeqNumberService seqNumberService
}

json(quoteList) { Quote quote ->
    id quote.id.toString()
    number seqNumberService.getFullNumber(quote)
    name quote.subject
    fullName seqNumberService.getFullName(quote)
}
